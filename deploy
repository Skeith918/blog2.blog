
#!/bin/bash

echo "Start deploy"
ssh -tq root@staticweb-02.etalab.gouv.fr "bash -lc 'cd /var/www/blog2.blog && git pull'"
ssh -tq root@staticweb-02.etalab.gouv.fr "bash -lc 'cd /var/www/blog2.blog && bundle exec jekyll doctor'"
ssh -tq root@staticweb-02.etalab.gouv.fr "bash -lc 'cd /var/www/blog2.blog && bundle exec jekyll build'"
echo "Deployed Successfully!"

exit 0
